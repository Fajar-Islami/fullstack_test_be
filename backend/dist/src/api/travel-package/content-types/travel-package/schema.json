{
    "kind": "collectionType",
    "collectionName": "travel_packages",
    "info": {
        "singularName": "travel-package",
        "pluralName": "travel-packages",
        "displayName": "travel_package"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "name": {
            "type": "string",
            "required": true,
            "minLength": 1
        },
        "description": {
            "type": "text",
            "required": true,
            "minLength": 10
        },
        "price": {
            "type": "biginteger",
            "required": true,
            "min": "1"
        },
        "image_travel": {
            "allowedTypes": [
                "images"
            ],
            "type": "media",
            "multiple": true
        },
        "order_detail_id": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::order-detail.order-detail",
            "inversedBy": "travel_package_id"
        }
    }
}
